{
    "name": "Marcar video youtube",
    "description": "Com este plugin vc pode marcar videos no youtube!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch?v=*"],
            "js": ["content.js", "background.js"],
            "css": ["style.css"]
        }
    ]
}